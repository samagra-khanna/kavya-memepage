<!DOCTYPE html>
<html lang="en">
<meta charset="utf-8" />
<link rel="stylesheet" href="assets/css/main.css" />
<link rel="stylesheet"
    href="https://fonts.googleapis.com/css2?family=Material+Symbols+Outlined:opsz,wght,FILL,GRAD@20..48,100..700,0..1,-50..200" />
<script src="https://kit.fontawesome.com/2bffe820e9.js" crossorigin="anonymous"></script>
<meta name="viewport"
    content="width=device-width, initial-scale=1.0, maximum-scale=1.0, shrink-to-fit=no, user-scalable=0" />
<title>Kavya-Memepage</title>
<meta name="description" content="" />
<meta itemprop="description" content="" />
<meta itemprop="image" content="URL_OF_DEPLOYMENT/branding/" />
<meta property="og:url" content="URL_OF_DEPLOYMENT" />
<meta property="og:type" content="website" />
<meta property="og:title" content="kavya-memepage" />
<meta property="og:description" content="" />
<meta property="og:image" content="URL_OF_DEPLOYMENT/branding/" />
<meta name="twitter:card" content="summary_large_image" />
<meta name="twitter:title" content="kavya-memepage" />
<meta name="twitter:description" content="" />
<meta name="twitter:image" content="URL_OF_DEPLOYMENT/branding/" />

<body>
    <div class="loader">
        <div class="loader-main"></div>
    </div>
    <div class="main-title">
        <div class="main-logo">
            <img src="/favicon.ico" alt="img" class="main-image" draggable="false">
            <h4 class="main-text">Kavya Memepage</h4>
        </div>
        <a href="/" class="main-upload"><span class="material-symbols-outlined">
                home
            </span></a>
    </div>
    <div class="file-wrapper">
        <div class="upload-btn-wrapper">
            <button class="btn upload-file font-weight-500">
                <span class="upload-btn">
                    <i class="fas fa-cloud-upload-alt font-50 pb-2" style="margin-right: 5px;"></i>
                    Upload image here
                </span>
                <span class="upload-select-button" id="blankFile">
                    Supports JPG, JPEG, PNG, GIF, JFIF
                </span>
                <span class="success">
                    <i class="far fa-check-circle text-success"></i>
                </span>
            </button>
            <form method="post" action="/upload" enctype="multipart/form-data">
                <input id="meme-post" onchange="checkFileFormat()" type="file" accept="image/*" name="meme-post"
                    required="true" />
            </form>
        </div>
    </div>
    <script src="https://code.jquery.com/jquery-3.5.1.min.js" defer></script>
    <script type="text/javascript" src="assets/js/main.js" defer></script>
</body>

</html>